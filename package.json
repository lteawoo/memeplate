{
  "name": "memeplate-workspace",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "dev": "pnpm -r --parallel --filter memeplate-web --filter memeplate-api dev",
    "dev:web": "pnpm --filter memeplate-web dev",
    "dev:api": "pnpm --filter memeplate-api dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter memeplate-web build",
    "build:api": "pnpm --filter memeplate-api build",
    "start": "pnpm --filter memeplate-api start",
    "preview": "pnpm --filter memeplate-web preview",
    "lint": "pnpm --filter memeplate-web lint",
    "typecheck": "pnpm -r --if-present typecheck"
  }
}
